#include <stdio.h>

void print_to_98(int n)
{
	int k;

	if (n <= 98)
	{
		for (k = n; k <= 98; k++)
		{
			printf("%d",k);
			if (k < 98)
				{
					putchar(',');
					putchar(' ');
				}
		}
	}
	else
	{
		for (k = n; k >= 98; k--)
		{
			printf("%d",k);
			if (k >= 98)
			{
				putchar(',');
				putchar(' ');
			}

		}
	}
	return;
}
