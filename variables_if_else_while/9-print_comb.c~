#include <stdio.h>
/**
 *Excercise nine - project: C - Variables, if, else, while
 *Description: Prints all possible combinations of single digit number
 *Return: Always (0);
 */
int main(void)
{
	int number;
	for (number = 0; number < 9; number++)
	{
		putchar(number + '0');
		//putchar(',');
		//putchar(' ');
	}
	putchar(9);
	return (0);
}
